{
  "name": "question-converter",
  "version": "0.0.1",
  "description": "Convert quiz questions from CanvasLMS api to custom yaml format",
  "main": "index.js",
  "repository": "https://github.com/mobalt/question-converter",
  "author": "Moises Baltazar <me@moisesb.com>",
  "scripts": {
    "lint": "prettier-eslint '{src,test}/**/*.*' --list-different --write",
    "lint:check": "prettier-eslint '{src,test}/**/*.*' --list-different; exit 0",
    "build": "babel src --out-dir=dist --source-maps",
    "dev": "babel-node ./src/index.js",
    "start": "nodemon --exec babel-node ./src/index.js",
    "test": "mocha --require esm"
  },
  "dependencies": {
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.test.js",
        "options": {
          "semi": true
        }
      },
      {
        "files": "*.{yml,yaml,json}",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
}
